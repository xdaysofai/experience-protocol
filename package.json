{
  "name": "experience-protocol",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "i": "pnpm install",
    "build": "pnpm -C contracts build && pnpm -C apps/web build",
    "dev:web": "pnpm -C apps/web dev",
    "dev:relayer": "pnpm -C apps/relayer dev",
    "test:chrome": "node test-experience.js"
  },
  "workspaces": [
    "contracts",
    "apps/web",
    "apps/relayer",
    "packages/sdk"
  ],
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "selenium-webdriver": "^4.35.0",
    "tailwindcss": "^4.1.13"
  }
}
